{
  "name": "aave-monitoring",
  "version": "1.0.0",
  "description": "A Monitoring DApp For Data Lovers",
  "main": "index.tsx",
  "author": "Eswara Sai",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --fix './src/**/*.{ts,tsx}'",
    "prettier:ts": "prettier --single-quote --parser typescript --write \"./src/**/*.{ts,tsx}\"",
    "compile-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css",
    "minify-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.min.css --output-style compressed",
    "map-sass": "node-sass src/assets/scss/argon-dashboard-react.scss src/assets/css/argon-dashboard-react.css --source-map true"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "chart.js": "2.9.1",
    "classnames": "2.2.6",
    "moment": "2.24.0",
    "node-sass": "4.13.0",
    "nouislider": "14.0.3",
    "react": "16.11.0",
    "react-chartjs-2": "2.8.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-datetime": "2.16.3",
    "react-dom": "16.11.0",
    "react-google-maps": "9.4.5",
    "react-router-dom": "5.1.2",
    "react-scripts": "3.2.0",
    "reactstrap": "8.1.1"
  },
  "devDependencies": {
    "@types/chart.js": "^2.8.10",
    "@types/classnames": "^2.2.9",
    "@types/googlemaps": "3.38.0",
    "@types/markerclustererplus": "2.1.33",
    "@types/node": "^12.12.0",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@types/react-router-dom": "^5.1.1",
    "@types/reactstrap": "^8.0.6",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "redux-logger": "^3.0.6",
    "typescript": "^3.6.4"
  },
  "lint-staged": {
    "./src/**/*.{ts,tsx}": [
      "yarn run prettier:ts",
      "yarn run lint:fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
